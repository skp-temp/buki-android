<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="Toolbar">
        <attr name="title_text" format="reference|string" />
        <attr name="title_center" format="boolean" />
        <attr name="back_button_visibility" format="boolean" />
        <attr name="action_button_left_visibility" format="boolean" />
        <attr name="action_button_right_visibility" format="boolean" />
        <attr name="action_button_left_text" format="reference|string" />
        <attr name="action_button_right_text" format="reference|string" />
    </declare-styleable>

    <declare-styleable name="InputBox">
        <attr name="hint" format="reference|string" />
        <attr name="maxLength" format="reference|integer" />
    </declare-styleable>

    <declare-styleable name="LargeButton">
        <attr name="text" format="reference|string" />
    </declare-styleable>

    <declare-styleable name="CharmTypeTag">
        <attr name="small" format="boolean" />
        <attr name="incomplete" format="boolean" />
    </declare-styleable>

    <declare-styleable name="BukiProgressBar">
        <attr name="radius" format="reference|integer" />
    </declare-styleable>
</resources>